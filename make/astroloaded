#
#   tries to report what the current user environment contains
#
#   alias whatsup 'source ~/bin/whatsup'
#

if ($?AIPSPATH) then
   echo "AIPS++  : $AIPSPATH"
endif

if ($?gip_root) then
   echo "GIPSY   : $gip_root"
endif 

if ($?KARMABASE) then
   echo "KARMA   : $KARMABASE - version $KARMA_VERSION"
endif

if ($?MIR) then
   echo "MIRIAD  : $MIR - version `cat $MIR/VERSION`"
endif

if ($?NEMO) then
   echo "NEMO    : $NEMO - version $NEMOVER"
endif

if ($?PGPLOT_DIR) then
   echo "PGPLOT  : $PGPLOT_DIR"
endif

if ($?SMPATH) then
   echo "SM      : $SMPATH"
endif

if ($?WASP2) then
   echo "WASP2   : $WASP2
endif


if ($?STARLAB_PATH) then
   echo "STARLAB : $STARLAB_PATH"
endif

if ($?ZENOPATH) then
   echo "ZENO    : $ZENO "
endif

if ($?LD_LIBRARY_PATH) then
   echo "LD_LIBRARY_PATH : $LD_LIBRARY_PATH"
endif


if ($?LD_PRELOAD) then
   echo "LD_PRELOAD      : $LD_PRELOAD"
endif

if ($?MANPATH) then
   echo "MANPATH  D      : $MANPATH"
endif







